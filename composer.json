{
    "name"             : "kallbuloso/notify",
    "license"          : "MIT",
    "description"      : "toastr.js, pnotify.js flush notifications for Laravel 5, 6, 7 and Lumen",
    "homepage"         : "https://github.com/kallbuloso/notify",
    "keywords"         : [
        "toastr",
        "pnotify",
        "toastr js",
        "pnotify js",
        "notification",
        "laravel",
        "laravel 5",
        "laravel 5.6",
        "laravel 5.7",
        "laravel 5.8",
        "laravel 6",
        "laravel 7",
        "laravel notification",
        "laravel toastr",
        "laravel pnotify",
        "flush",
        "flush notifications",
        "laravel flush notifications",
        "flush messages",
        "messages",
        "alerts",
        "Lumen",
        "php",
        "kallbuloso"
    ],
    "authors"          : [
        {
            "name"    : "Amaral karl",
            "email"   : "kallbuloso@gmail.com",
            "homepage": "https://github.com/kallbuloso/notify",
            "role"    : "Developer"
        }
    ],
    "require"          : {
        "php"               : ">=7.0",
        "illuminate/session": "^5.5 || ^6.0 || ^7.0",
        "illuminate/support": "^5.5 || ^6.0 || ^7.0"
    },
    "require-dev"      : {
        "orchestra/testbench": "^2.0.4 || ^3.0 || ^4.0 || ^5.0",
        "phpunit/phpunit"    : "^4.8.36 || ^5.7 || ^6.0 || ^7.0 || ^8.3 || ^9.0"
    },
    "autoload"         : {
        "psr-4": {
            "kallbuloso\\Notify\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev"     : {
        "psr-4": {
            "kallbuloso\\Notify\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable"    : true,
    "extra"            : {
        "laravel": {
            "providers": [
                "kallbuloso\\Notify\\NotifyServiceProvider"
            ],
            "aliases"  : {
                "Notify": "kallbuloso\\Notify\\Facades\\Notify"
            }
        }
    },
    "config"           : {
        "sort-packages": true
    }
}
